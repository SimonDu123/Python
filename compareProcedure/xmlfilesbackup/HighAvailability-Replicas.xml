               <queryConfiguration callableName="" connectionName="SQLServerConnection" isScript="false" minimumRefreshInterval="10000">
                  <properties/>
                  <guardExpressionText></guardExpressionText>
                  <commandText><![CDATA[declare @dbaddress  NVARCHAR(4000);
declare @dbaddress2  NVARCHAR(100);
declare @portno nvarchar(6)
set @dbaddress='{{dbaddress}}'
set @dbaddress2 = case
			when charindex('.',@dbaddress) > 0 then lower(substring(@dbaddress, 1, charindex('.',@dbaddress) - 1)) 
			when charindex(',',@dbaddress) > 0 then lower(substring(@dbaddress, 1, charindex(',',@dbaddress) - 1))
		else (lower(@dbaddress))
		end
set @portno = case
			when charindex(',',@dbaddress) > 0 then substring(@dbaddress, charindex(',',@dbaddress) + 1,20)
		else null
		end
select 
	r.replica_id, 
	r.group_id,
	r.replica_metadata_id,
	r.replica_server_name,
	r.owner_sid,
	r.endpoint_url,
	r.availability_mode,
	r.availability_mode_desc,
	r.failover_mode,
	r.failover_mode_desc,
	r.session_timeout, 
	r.primary_role_allow_connections,
	r.primary_role_allow_connections_desc,
	r.secondary_role_allow_connections,
	r.secondary_role_allow_connections_desc,
	r.create_date,
	r.modify_date,
	r.backup_priority,
	r.read_only_routing_url,
	s.is_local,
	s.role,
	s.role_desc,
	s.operational_state,
	s.operational_state_desc,
	s.connected_state,
	s.connected_state_desc,
	s.recovery_health,
	s.recovery_health_desc,
	s.synchronization_health,
	s.synchronization_health_desc,
	s.last_connect_error_number,
	s.last_connect_error_description,
	s.last_connect_error_timestamp
from 
	sys.availability_replicas r
join sys.dm_hadr_availability_replica_states s on r.replica_id = s.replica_id
join sys.availability_group_listeners l on l.group_id = r.group_id
and (lower(l.dns_name)=@dbaddress2 or charindex(@dbaddress2,l.ip_configuration_string_from_cluster) > 0)
	and (@portno is null or l.port=@portno)]]></commandText>
